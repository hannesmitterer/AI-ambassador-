<script>
    // 1. DATA CONFIGURATION
    const modules = [
        // AIC Track (Backend Logic)
        { id: 'aic1', track: 'aic', name: 'Phase 1: CRE v2', start: 0, dur: 1, status: 'done', 
          desc: 'Probabilistische Priorisierung, NSR Integration. <br>‚úÖ Grundimplementierung abgeschlossen.' },
        { id: 'aic2', track: 'aic', name: 'Phase 2: Dynamic Loop', start: 1, dur: 1.5, status: 'dev', 
          desc: 'Council Utility Scores, TFK-Puls Visualisierung. <br>‚öôÔ∏è Aktuell in Entwicklung.' },
        { id: 'aic3', track: 'aic', name: 'Phase 3: Dry-Run Engine', start: 2.5, dur: 2, status: 'future', 
          desc: 'Automatisierte Konfliktsimulation vor Output. <br>üîú Geplant f√ºr Q2.' },
        { id: 'aic4', track: 'aic', name: 'Phase 4: Full Autonomy', start: 4.5, dur: 3, status: 'future', 
          desc: 'Selbstregulierende Agenten-Schw√§rme ohne manuellen Eingriff.' },

        // GGI Track (Frontend Interface)
        { id: 'ggi1', track: 'ggi', name: 'Phase 1: UI Base', start: 0.5, dur: 1.5, status: 'done', 
          desc: 'Dashboard-Struktur, Node-Visualisierung. <br>‚úÖ Basis steht.' },
        { id: 'ggi2', track: 'ggi', name: 'Phase 2: Live Feedback', start: 2, dur: 2, status: 'dev', 
          desc: 'Echtzeit-Intervention, Vetorecht-Buttons. <br>‚öôÔ∏è Mockup aktiv.' },
        { id: 'ggi3', track: 'ggi', name: 'Phase 3: Audit Log', start: 4, dur: 2, status: 'future', 
          desc: 'Unver√§nderliche Blockchain-Logs der Entscheidungen.' },
        { id: 'ggi4', track: 'ggi', name: 'Phase 4: Council VR', start: 6, dur: 3, status: 'future', 
          desc: 'Immersive Konferenzraum-Visualisierung f√ºr komplexe Entscheidungen.' }
    ];

    const dependencies = [
        { from: 'aic1', to: 'ggi1', type: 'normal' },
        { from: 'aic2', to: 'ggi2', type: 'critical' },
        { from: 'aic3', to: 'ggi3', type: 'normal' },
        { from: 'ggi1', to: 'ggi2', type: 'normal' },
        { from: 'aic2', to: 'aic3', type: 'critical' }
    ];

    // 2. RENDER LOGIC
    const lanes = {
        aic: document.getElementById('lane-aic'),
        ggi: document.getElementById('lane-ggi')
    };
    const tooltip = document.getElementById('tooltip');
    const svg = document.getElementById('connections');
    const moduleEls = {};

    function renderGantt() {
        modules.forEach(mod => {
            const el = document.createElement('div');
            el.className = `module status-${mod.status}`;
            el.textContent = mod.name;
            // Width calculation: (Duration / 12 Months) * 100%
            el.style.left = `${(mod.start / 12) * 100}%`;
            el.style.width = `${(mod.dur / 12) * 100}%`;
            
            // Interaction
            el.addEventListener('mousemove', (e) => {
                tooltip.style.display = 'block';
                tooltip.style.left = e.pageX + 15 + 'px';
                tooltip.style.top = e.pageY + 15 + 'px';
                tooltip.innerHTML = `<h4>${mod.name}</h4><p>${mod.desc}</p>`;
            });
            el.addEventListener('mouseleave', () => {
                tooltip.style.display = 'none';
            });

            lanes[mod.track].appendChild(el);
            moduleEls[mod.id] = el;
        });

        // Delay drawing lines to ensure DOM elements have dimensions
        setTimeout(drawLines, 100);
    }

    function drawLines() {
        // Clear previous lines on resize
        svg.innerHTML = '';
        
        const wrapperRect = document.querySelector('.gantt-wrapper').getBoundingClientRect();
        
        dependencies.forEach(dep => {
            const fromEl = moduleEls[dep.from];
            const toEl = moduleEls[dep.to];
            
            if(!fromEl || !toEl) return;

            const fromRect = fromEl.getBoundingClientRect();
            const toRect = toEl.getBoundingClientRect();

            // Calculate Start Point (Bottom Center of 'From')
            const x1 = (fromRect.left - wrapperRect.left) + fromRect.width / 2;
            const y1 = (fromRect.top - wrapperRect.top) + fromRect.height;
            
            // Calculate End Point (Top Center of 'To')
            const x2 = (toRect.left - wrapperRect.left) + toRect.width / 2;
            const y2 = (toRect.top - wrapperRect.top);

            const line = document.createElementNS('http://www.w3.org/2000/svg', 'line');
            line.setAttribute('x1', x1);
            line.setAttribute('y1', y1);
            line.setAttribute('x2', x2);
            line.setAttribute('y2', y2);
            
            // Style logic
            if(dep.type === 'critical') {
                 line.setAttribute('stroke', 'var(--alert-color)');
                 line.setAttribute('stroke-dasharray', '5,5');
            } else {
                 line.setAttribute('stroke', '#555');
                 line.setAttribute('stroke-dasharray', '4');
            }

            svg.appendChild(line);
        });
    }

    // Source Code Modal Logic
    function toggleSourceModal() {
        const modal = document.getElementById('source-modal');
        const textarea = document.getElementById('full-source-code');
        
        if (modal.classList.contains('hidden-modal')) {
            // Fill with current HTML
            textarea.value = "<!DOCTYPE html>\n" + document.documentElement.outerHTML;
            modal.classList.remove('hidden-modal');
            modal.classList.add('visible-modal');
        } else {
            modal.classList.remove('visible-modal');
            modal.classList.add('hidden-modal');
        }
    }

    function copyFullSource() {
        const sourceText = document.getElementById('full-source-code');
        sourceText.select();
        navigator.clipboard.writeText(sourceText.value);
        alert("Code copied to clipboard!");
    }

    // 3. INITIALIZE
    renderGantt();

    // Redraw lines when window is resized to keep connections correct
    window.addEventListener('resize', drawLines);

</script>


</body></html>. </p>

